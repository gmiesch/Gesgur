{
  "name": "Myo Imgur",
  "description": "Interactive imgur mode for the myo arm band",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["mystyles.css"],
      "js": ["myoimgur.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://api.twitter.com/*"
  ],
  "manifest_version": 2
}
